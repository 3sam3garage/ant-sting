{
  "name": "ant-sting",
  "version": "0.0.1",
  "description": "ant-sting",
  "author": "3sam3",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:agent": "nest build agent",
    "build:batch": "nest build batch",
    "build:consumer": "nest build consumer",
    "batch:ei:naver": "nest start batch -- economic-information scrape-naver",
    "batch:ei:kcif": "nest start batch -- economic-information scrape-kcif",
    "batch:ei:analyze": "nest start batch -- economic-information analyze",
    "batch:poly-market:poll": "nest start batch -- poly-market poll",
    "batch:poc:graph": "nest start batch -- poc graph",
    "agent": "nest start agent --watch",
    "consumer": "nest start consumer --watch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@google/genai": "^0.6.1",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.8.3",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.6",
    "date-fns": "^3.6.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "nest-commander": "^3.14.0",
    "node-html-parser": "^6.1.13",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "user-agents": "^1.1.428",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.0",
    "@types/unzipper": "^0.10.10",
    "@types/user-agents": "^1.0.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@libs/domain(|/.*)$": "<rootDir>/libs/domain/src/$1",
      "^@libs/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@libs/ai(|/.*)$": "<rootDir>/libs/ai/src/$1",
      "^@libs/external-api(|/.*)$": "<rootDir>/libs/external-api/src/$1",
      "^@libs/browser(|/.*)$": "<rootDir>/libs/browser/src/$1"
    }
  }
}
