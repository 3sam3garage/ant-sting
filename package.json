{
  "name": "ant-sting",
  "version": "0.0.1",
  "description": "ant-sting",
  "author": "3sam3",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:api": "nest start api --watch",
    "start:api:prod": "node dist/apps/api/main",
    "start:batch:stock-report:scrape-naver": "nest start batch -- stock-report scrape-naver",
    "start:batch:stock-report:scrape-hana": "nest start batch -- stock-report scrape-hana",
    "start:batch:stock-report:scrape-shinhan": "nest start batch -- stock-report scrape-shinhan",
    "start:batch:stock-report:scrape-kiwoom": "nest start batch -- stock-report scrape-kiwoom",
    "start:batch:stock:scrape-sec-ticker": "nest start batch -- stock scrape-sec-ticker",
    "start:batch:economic-inforamtion:scrape-naver": "nest start batch -- economic-information scrape-naver",
    "start:batch:economic-inforamtion:scrape-kcif": "nest start batch -- economic-information scrape-kcif",
    "start:batch:economic-inforamtion:exchange-rate": "nest start batch -- economic-information exchange-rate",
    "start:batch:economic-inforamtion:bond-yield": "nest start batch -- economic-information bond-yield",
    "start:batch:economic-inforamtion:interest-rate": "nest start batch -- economic-information interest-rate",
    "start:batch:economic-inforamtion:stock-index": "nest start batch -- economic-information stock-index",
    "start:batch:economic-inforamtion:analyze": "nest start batch -- economic-information analyze",
    "start:batch:test": "nest start batch -- test test2",
    "start:consumer": "nest start consumer --watch",
    "start:agent": "nest start agent --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/api/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.679.0",
    "@aws-sdk/client-bedrock-runtime": "^3.679.0",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@slack/web-api": "^7.7.0",
    "axios": "^1.7.3",
    "bull": "^4.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.6",
    "date-fns": "^3.6.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "nest-commander": "^3.14.0",
    "node-html-parser": "^6.1.13",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "unzipper": "^0.12.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.0",
    "@types/unzipper": "^0.10.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@libs/domain(|/.*)$": "<rootDir>/libs/domain/src/$1",
      "^@libs/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@libs/ai(|/.*)$": "<rootDir>/libs/ai/src/$1",
      "^@libs/external-api(|/.*)$": "<rootDir>/libs/external-api/src/$1"
    }
  }
}
